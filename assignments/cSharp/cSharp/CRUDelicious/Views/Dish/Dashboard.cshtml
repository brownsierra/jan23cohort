@{
    ViewData["Title"] = "Home Page";
}
@model List<Dish>


<h1 class="crudheader">Welcome to CRUDelicious</h1>
<nav id="nav">
    <a href="dashboard" id="homehome">Home</a>
    <h2>|</h2>
    <a href="addDish" id="homeadddish">Add a Dish</a>
</nav>
<h3>Check out some recent dishes</h3>
<hr>
@{
    foreach(Dish d in Model) {
        <h2>
            <a href="dish/@d.DishId/view">@d.Name</a>
            by @d.Chef
        </h2>
    }
}